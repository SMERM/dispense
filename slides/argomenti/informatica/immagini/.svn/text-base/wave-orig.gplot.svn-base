set term postscript eps monochrome
set samples 5000
set xzeroaxis
set yzeroaxis
#
#
frq=300
max=32768
pi2=pi*2
plot [-0.005:0.02] ((max*sin(pi2*frq*x))+(pi/3))+\
		   (((max/2)*sin(pi2*2*frq*x))+(pi/7))+\
		   ((max/3)*sin(pi2*3*frq*x)) notitle
