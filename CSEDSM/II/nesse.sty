\ProvidesPackage{nesse}
\usepackage{amsmath,amssymb,mathrsfs,multirow}
\usepackage{graphicx}
%\usepackage{natbib}
\newcommand{\E}{\text{E}}
\newcommand{\independ}{\perp \! \! \! \perp}
\newcommand{\notindepend}{\independ \! \! \! \! \! \! / \hspace{2mm}}
\newcommand{\iid}{\text{ \raisebox{2pt}{$\sim$}} \! \! \! \! \! \!\text{\raisebox{-1pt}{\tiny{i.i.d.}}}}
\newcommand{\var}{\text{Var}}
\newcommand{\cor}{\text{Cor}}
\newcommand{\cov}{\text{Cov}}
\newcommand{\raiseto}[1]{\to \!\!\!\!\!\!\raisebox{8pt}{{\scriptsize $#1$}} \hspace{5pt}}
\newcommand{\D}[2]{\frac{\partial #2}{\partial #1}}
\newcommand{\DD}[2]{\frac{\partial^2 #2}{\partial #1^2}}
\newcommand{\Dn}[3]{\frac{\partial^#3 #2}{\partial #1^#3}}
